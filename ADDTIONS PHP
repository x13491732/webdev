<div id="products" class="container-fluid text-center bg-grey">
  <h2>Games</h2><br>
  <div class="row text-center slideanim">
    <div align="center">
<div id='showGAME'></div><br>
<input type="button" onclick="previous()" value="<<">
<input type="button" onclick="next()" value=">>">

<script>
var i = 0;
displayGAME(i);

function displayGAME(i) {
    var xmlhttp = new XMLHttpRequest();
    xmlhttp.onreadystatechange = function() {
        if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
            myFunction(xmlhttp, i);
        }
    };
    xmlhttp.open("GET", "GAMESLIST.xml", true);
    xmlhttp.send();
}

function myFunction(xml, i) {
    var xmlDoc = xml.responseXML; 
    x = xmlDoc.getElementsByTagName("GAME");
    document.getElementById("showGAME").innerHTML =
    "Console: " +
    x[i].getElementsByTagName("CONSOLE")[0].childNodes[0].nodeValue +
    "<br>Title: " +
    x[i].getElementsByTagName("TITLE")[0].childNodes[0].nodeValue +
    "<br>Price: " + 
    x[i].getElementsByTagName("PRICE")[0].childNodes[0].nodeValue;
}

function next() {
if (i < x.length-1) {
  i++;
  displayGAME(i);
  }
}

function previous() {
if (i > 0) {
  i--;
  displayGAME(i);
  }
}
</script>


